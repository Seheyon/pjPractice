<Window x:Class="pjPractice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pjPractice"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="500">
    <Grid>
        <Grid.ColumnDefinitions >
            <ColumnDefinition/>
            <ColumnDefinition Width="5*"/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="5*"/>
        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="1">
            <Button Click="SearchWeb" x:Name="NewSetting" Content="새로고침" Height="30"/>
        </StackPanel>

        <StackPanel Grid.Row="1" Grid.Column="1">
            <Button x:Name="notice1" Content="" Height="30"/>
            <Button x:Name="notice2" Content="" Height="30"/>
            <Button x:Name="notice3" Content="" Height="30"/>
            <Button x:Name="notice4" Content="" Height="30"/>
            <Button x:Name="notice5" Content="" Height="30"/>
            <Button x:Name="notice6" Content="" Height="30"/>
            <Button x:Name="notice7" Content="" Height="30"/>
            <Button x:Name="notice8" Content="" Height="30"/>
            <Button x:Name="notice9" Content="" Height="30"/>
            <StackPanel Orientation="Horizontal">
                <Button x:Name="leftButton" Width="40" Height="30" Content="left"/>
                <Button x:Name="rightButton" Width="40" Height="30" Content="right"/>
            </StackPanel>
        </StackPanel>
        
        

    </Grid>
</Window>
